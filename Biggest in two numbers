#include<iostream>
using namespace std;
class biggest
{
	int a,b;
	public:
		void input();
		void display();
};
void biggest::input(){
	cout<<"enter first number:";
	cin>>a;
	cout<<"enter second number:";
	cin>>b;
}
void biggest::display(){
	if(a>b)
	{
		cout<<"a is the biggest";
	}
	else{
		cout<<"b is the biggest";
	}
}
int main()
{
	biggest x;
	x.input();
	x.display();
	return 0;
}
